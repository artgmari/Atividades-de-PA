algoritmo "semnome"

var
 salario,renda: logico
inicio
escreval("Qual seu salraio")
leia(salario)

 se(salario < = 1903.98)entao
   renda <- 0
   fimse

 se((salrio> = 1903.99) e (salrio <= 2826.65)) entao
    renda <- salario vezes 0.075
   fimse
 
 escreval("O desconto na sua folha de pagamento sera de: R$", renda)
fimalgoritmo
